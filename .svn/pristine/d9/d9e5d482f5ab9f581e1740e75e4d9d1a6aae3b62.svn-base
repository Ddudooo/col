package com.col.channel.dao;

public class FollowVO {
	
	private String followChannel;
	
	//follower
	private String myFollowerID;
	private String myFollowerNick;
	private String myFollowerImg;
	private int myFollowerCnt;
	
	//following
	private String myFollowingID;
	private String myFollowingNick;
	private String myFollowingImg;
	private int myFollowingCnt;
	
	//getter & setter
	public String getFollowChannel() {
		return followChannel;
	}
	public void setFollowChannel(String followChannel) {
		this.followChannel = followChannel;
	}
	public String getMyFollowerID() {
		return myFollowerID;
	}
	public void setMyFollowerID(String myFollowerID) {
		this.myFollowerID = myFollowerID;
	}
	public String getMyFollowerNick() {
		return myFollowerNick;
	}
	public void setMyFollowerNick(String myFollowerNick) {
		this.myFollowerNick = myFollowerNick;
	}
	public String getMyFollowerImg() {
		return myFollowerImg;
	}
	public void setMyFollowerImg(String myFollowerImg) {
		this.myFollowerImg = myFollowerImg;
	}
	public int getMyFollowerCnt() {
		return myFollowerCnt;
	}
	public void setMyFollowerCnt(int myFollowerCnt) {
		this.myFollowerCnt = myFollowerCnt;
	}
	public String getMyFollowingID() {
		return myFollowingID;
	}
	public void setMyFollowingID(String myFollowingID) {
		this.myFollowingID = myFollowingID;
	}
	public String getMyFollowingNick() {
		return myFollowingNick;
	}
	public void setMyFollowingNick(String myFollowingNick) {
		this.myFollowingNick = myFollowingNick;
	}
	public String getMyFollowingImg() {
		return myFollowingImg;
	}
	public void setMyFollowingImg(String myFollowingImg) {
		this.myFollowingImg = myFollowingImg;
	}
	public int getMyFollowingCnt() {
		return myFollowingCnt;
	}
	public void setMyFollowingCnt(int myFollowingCnt) {
		this.myFollowingCnt = myFollowingCnt;
	}

}
